{
  "name": "vitest-bdd-monorepo",
  "private": true,
  "version": "0.6.0",
  "description": "Monorepo for vitest-bdd plugin and related packages.",
  "homepage": "https://github.com/midasum/vitest-bdd",
  "repository": {
    "type": "git",
    "url": "https://github.com/midasum/vitest-bdd.git"
  },
  "scripts": {
    "build": "pnpm -r build",
    "canary": "./bin/publish.sh --canary",
    "beta": "./bin/publish.sh --beta",
    "format": "biome format --fix",
    "pub": "./bin/publish.sh",
    "test-app": "pnpm -filter test-app",
    "vitest-bdd": "pnpm -filter vitest-bdd",
    "test": "pnpm -r test",
    "pretest": "pnpm -r res:build",
    "test:watch": "pnpm -r test:watch"
  },
  "author": "Gaspard Anna Bucher <g.a.bucher@midasum.com>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.2.3"
  }
}
